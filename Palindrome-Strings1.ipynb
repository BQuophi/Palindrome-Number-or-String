    "# Python program that checks if a string is a palindrome \n",
    "string = input('Enter the string: ')\n",
    "\n",
    "if string == string[::-1] :\n",
    "    print('The string is a palindrome')\n",
    "else:\n",
    "    print('The string is not a palindrome')"
   ]
